{
  "name": "marcomag89/Graphene",
  "description": "PHP rest framework",
  "type": "framework",
  "repositories": [
    {
      "type": "package",
      "source": {
        "url": "git://github.com/marcomag89/Graphene.git",
        "type": "git",
        "reference": "composer-integrated"
      },
      "autoload": {
        "psr-0" : {
          "Graphene\\Graphene" : "src"
        }
      }
    }
