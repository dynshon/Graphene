    "doctrine/orm": "^2.5",
    "ptcong/php-cache-manager": "dev-master"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "GrapheneTeam",
      "email": "marcomagnetti@gmail.com"
    }
  ],
